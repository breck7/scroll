  get requirements() {
    const {requireOnce} = this
    if (this.root.alreadyRequired.has(requireOnce))
      return ""
    this.root.alreadyRequired.add(requireOnce)
    return requireOnce + "\n\n"
  }
  compile() {
    return this.requirements + this.compileInstance()
  }
