  setFile(file) {
   this.file = file
   return this
  }
  relativePathToCompileTarget = ""
  stack = []
  clearStack() {
   const result = this.stack.join("")
   this.stack = []
   return result
  }
  compileEmbeddedVersion() {
   return this.map(child => (child.compileEmbeddedVersion ? child.compileEmbeddedVersion() : child.compile())).join("\n")
  }
  get footnotes() {
   if (this._footnotes === undefined) this._footnotes = this.filter(node => node.isFootnote)
   return this._footnotes
  }
  file = {}
  get permalink() {
   return this.get("permalink") || this.file.permalink || ""
  }
