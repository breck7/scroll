 javascript
  compile(spaces) {
   const title = this.getContent()
   const permalink = this.getRootNode().get("permalink") || jtree.Utils.stringToPermalink(this.getContent())
   return `<h1><a href="${permalink}.html">${title}</a></h1>`
  }
